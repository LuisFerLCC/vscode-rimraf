root = true

[*]
charset = utf-8
end_of_line = crlf
indent_style = tab
indent_size = 4
max_line_length = 133
insert_final_newline = true
trim_trailing_whitespace = true
# noinspection EditorConfigKeyCorrectness
quote_type = double
ij_any_keep_blank_lines_in_declarations = 1
ij_any_keep_blank_lines_in_code = 1

[*.yml]
indent_style = space
indent_size = 2
